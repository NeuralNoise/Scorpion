module org.scorpion.lang;


def __init__(string** args)
{
  int i =0;
  int max = 100000000;
  bool go;
  asm("
       .lbl @loop
         inc i
         ;cout i cout '$n'
         ilt go i max
         jit loop go
         cout i cout ' ' cout (long) go cout '$n'
      ");
      
}

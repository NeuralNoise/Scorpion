package org.scorpion.lang;

class Starter 
{
   static def __init__(string[] args)
   {
     asm("
	     bconst @jump #0
         iconst @i #0
		 iconst @size_t #10
		 
		 .label @l_start
		   inc i
		   cout ('i = ' << i << '\n')
		   ilt jump, i,size_t
		   jit l_start, jump
	     ");
     return 1;
   }
}
